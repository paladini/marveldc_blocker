{
    "name": "Marvel-DC Blocker",
    "description": "Remove any shitty post about Marvel/DC universe from Facebook.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
	    "matches":["*://*.facebook.com/*"],
            "run_at": "document_start",
            "js":["scripts/facebook_filter.js"]
        }
    ]
}
